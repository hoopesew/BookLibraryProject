@{
    ViewData["Title"] = "Home Page";
}

@model BooksListViewModel

    <h1>Library</h1>
    
        @foreach (Project p in Model.Books)
    {
        <h3>@p.Title</h3>
        <ul>
            <li>Book Author: @p.Author</li>
            <li>Publisher: @p.Publisher</li>
            <li>ISBN: @p.ISBN</li>
            <li>Classification: @p.Classification</li>
            <li>Category: @p.Category</li>
            <li>PageCount: @p.PageCount</li>
            <li>Price: $@p.Price</li>

        </ul>
    }

<div page-model ="@Model.Paginationinfo" pageaction = "Index">

</div>

   